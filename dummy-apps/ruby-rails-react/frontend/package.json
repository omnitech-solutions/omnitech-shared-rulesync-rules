{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "turbo run dev:app --parallel",
    "dev:app": "vite",
    "build": "turbo run build:app",
    "build:app": "tsc -b && vite build",
    "lint": "turbo run lint:app",
    "lint:app": "eslint .",
    "preview": "turbo run preview:app",
    "preview:app": "vite preview",
    "api:gen": "orval --config orval.config.cjs"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "@reduxjs/toolkit": "^2.6.1",
    "@tanstack/react-query": "^5.66.0",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "autoprefixer": "^10.4.20",
    "orval": "^7.10.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "turbo": "^2.5.6",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
